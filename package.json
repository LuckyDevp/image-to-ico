{
  "name": "image-to-ico-converter",
  "version": "1.0.0",
  "description": "A simple application to convert images to ICO format",
  "main": "image-to-ico.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "keywords": [
    "electron",
    "icon",
    "converter",
    "ico"
  ],
  "author": "Source Developer",
  "license": "MIT",
  "dependencies": {
    "jimp": "^0.22.10",
    "to-ico": "^1.1.5"
  },
  "devDependencies": {
    "electron": "25.0.0",
    "electron-builder": "^24.4.0"
  },
  "build": {
    "appId": "com.example.image-to-ico",
    "productName": "Image to ICO Converter",
    "electronVersion": "25.0.0",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "mac": {
      "icon": "assets/icon.ico"
    },
    "linux": {
      "icon": "assets/icon.ico"
    }
  }
}
